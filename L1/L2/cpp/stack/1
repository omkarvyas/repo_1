#include <iostream>
using namespace std;

#define MAX 100



class Stack{

    int stk_ptr;
    int arr[MAX];

        public:
        Stack(){
            stk_ptr = -1;
    
         }
        void push(int data){
        
        cout<<"in push"<<endl;
        
        }
        int pop(){
        
        
        cout<<"in pop"<<endl;

        }
        bool isempty(){
        
        
        cout<<"in isempty"<<endl;
        }
        bool isfull(){
        
        
        cout<<"in isfull"<<endl;
        
        }




};

int main(){
    Stack mystack;

    mystack.push(10);
        
    return 0;

}
